---
import DashboardLayout from "../../components/dashboard/DashboardLayout.astro";
import SkeletonAnalyticsSystem from "../../components/dashboard/SkeletonAnalyticsSystem";
---

<DashboardLayout title="Analytics (Demo View)" user={null} imagingCenter={null}>
  <div
    class="bg-yellow-100 border-l-4 border-yellow-500 text-yellow-900 p-4 rounded mb-6"
    role="alert"
  >
    <p class="font-bold">Demo Mode</p>
    <p>
      This is a demonstration of the analytics feature. Complete your PSA to
      unlock real-time performance and revenue data.
    </p>
  </div>
  <SkeletonAnalyticsSystem client:load />
</DashboardLayout>
